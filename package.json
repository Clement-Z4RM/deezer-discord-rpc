{
  "name": "deezer-discord-rpc",
  "version": "1.1.1",
  "description": "A Discord RPC for Deezer",
  "main": "./build/src/index.js",
  "scripts": {
    "start": "electron ./build/src/index.js",
    "build": "yarn build:ts && yarn icon && yarn build:exe",
    "build:ts": "tsc --resolveJsonModule && cpy ./src/prompt.html build/",
    "build:exe": "electron-builder",
    "run": "yarn build:ts && yarn start",
    "icon": "png-to-ico src/img/icon.png > src/img/icon.ico && cd ./build/src && mkdir img && png-to-ico ../../src/img/icon.png > img/icon.ico"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "4.1.2",
    "discord-rpc": "^4.0.1",
    "ua-parser-js": "^1.0.33",
    "ws": "^8.12.1"
  },
  "devDependencies": {
    "@types/discord-rpc": "^4.0.0",
    "@types/node": "^18.7.6",
    "@types/ua-parser-js": "^0.7.36",
    "@types/ws": "^8.5.4",
    "cpy-cli": "^4.2.0",
    "electron": "^23.1.0",
    "electron-builder": "^23.6.0",
    "png-to-ico": "^2.1.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "repository": "https://github.com/JustYuuto/deezer-discord-rpc.git",
  "author": "NetherMC",
  "license": "MIT",
  "build": {
    "appId": "com.github.yuuto.deezerdiscordrpc",
    "productName": "Deezer Discord RPC",
    "protocols": [
      {
        "name": "deezer-discord-rpc",
        "schemes": [
          "deezer-discord-rpc"
        ]
      }
    ],
    "files": [
      "!src/*",
      "!src/**/*",
      "build/package.json",
      "build/src/*",
      "build/src/**/*"
    ]
  }
}
