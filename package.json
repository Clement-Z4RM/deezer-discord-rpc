{
  "name": "deezer-discord-rpc",
  "version": "1.0.8",
  "description": "A Discord RPC for Deezer",
  "main": "./src/index.js",
  "scripts": {
    "start": "electron ./dist/src/index.js",
    "build": "yarn build:ts && yarn icon && yarn install --prod --non-interactive --cwd ./dist && yarn build:exe",
    "build:ts": "tsc --resolveJsonModule",
    "build:exe": "electron-packager dist/ DeezerDiscordRPC --platform=win32 --arch=x64",
    "run": "yarn build:ts && yarn start",
    "icon": "png-to-ico src/img/icon.png > src/img/icon.ico && cd ./dist/src && mkdir img && png-to-ico ../../src/img/icon.png > img/icon.ico"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "discord-rpc": "^4.0.1"
  },
  "devDependencies": {
    "@types/discord-rpc": "^4.0.0",
    "@types/node": "^18.7.6",
    "electron": "^23.1.0",
    "electron-packager": "^17.1.1",
    "png-to-ico": "^2.1.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "repository": "https://github.com/JustYuuto/deezer-discord-rpc.git",
  "author": "NetherMC",
  "license": "MIT"
}
